DISBIN=$ROOT/dis/lib

<$ROOT/mkfiles/mkdis
<$ROOT/mkfiles/mksubdirs

$DISBIN/arg.dis: arg.dis
	rm -f $DISBIN/arg.dis && cat arg.dis > $DISBIN/arg.dis

$DISBIN/readdir.dis: readdir.dis
	rm -f $DISBIN/readdir.dis && cat readdir.dis > $DISBIN/readdir.dis
	
